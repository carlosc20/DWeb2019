extends layout

block content
    .w3-container
        .w3-card-4
            header.w3-container
                h1 Entidades
                table.w3-table-all
                    tr  
                        th Designação
                        th Sigla
                        th Internacional
                    each e in lista
                        tr  
                            td 
                                a(href='/' + e.id)= e.designacao 
                            td= e.sigla
                            td= e.internacional